{
  "name": "montech-assignment",
  "version": "1.0.0",
  "description": "Montech Backend Assignment",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production npx babel-node ./src/index.js",
    "start:dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/index.js",

    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test | coveralls",
    "test": "nyc mocha --require @babel/register 'test/**/**test.js' --timeout 500000 --exit",

    "createtables": "cross-env npx sequelize db:migrate",
    "inserttables": "cross-env npx sequelize db:seed:all",
    "deletetables": "cross-env npx sequelize db:migrate:undo:all",

    "insertOnetable": "cross-env npx sequelize db:seed --seed 20221310-demo-movie.js",
    "createOnetable": "cross-env npx sequelize-cli db:migrate --to 20221310-create-movie.js",
    "deleteOnetable": "cross-env npx sequelize db:migrate:undo --name 20221310-create-movie.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/key-joshua/montech-assignment"
  },
  "keywords": [
    "Assigment"
  ],
  "author": "Josue",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/register": "^7.18.9",
    "babel-loader": "^8.2.5",
    "babel-node": "^0.0.1-security",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "codecov": "^3.8.3",
    "cors": "^2.8.5",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "http-status": "^1.5.3",
    "joi": "^17.6.3",
    "mocha": "^10.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "nodemon": "^1.3.3"
  }
}
